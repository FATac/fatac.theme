<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
    default_skin="fatac.theme">

    <object name="fatac_theme_custom_images"
        meta_type="Filesystem Directory View"
        directory="fatac.theme:skins/fatac_theme_custom_images"/>
    <object name="fatac_theme_custom_templates"
        meta_type="Filesystem Directory View"
        directory="fatac.theme:skins/fatac_theme_custom_templates"/>
    <object name="fatac_theme_styles"
        meta_type="Filesystem Directory View"
        directory="fatac.theme:skins/fatac_theme_styles"/>

    <skin-path name="fatac.theme" based-on="Plone Default">
        <layer name="fatac_theme_custom_images" insert-after="custom"/>
        <layer name="fatac_theme_custom_templates" insert-after="fatac_theme_custom_images"/>
        <layer name="fatac_theme_styles" insert-after="fatac_theme_custom_templates"/>
        <layer name="sunburst_images" insert-after="fatac_theme_styles"/>
        <layer name="sunburst_templates" insert-after="sunburst_images"/>
        <layer name="sunburst_styles" insert-after="sunburst_templates"/>
    </skin-path>

</object>
