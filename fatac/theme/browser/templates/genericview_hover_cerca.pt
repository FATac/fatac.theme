<div class="genericview_hover_cerca" tal:define="dades_objecte view/getDadesObjecte">
    <tal:cond tal:condition="dades_objecte">
        <a tal:attributes="href string:${here/portal_url}/legalValidation?objectIdsVal=${dades_objecte/id}">
            <img class="miniatura_objecte" tal:attributes="src view/getThumbnailObjecte; alt dades_objecte/titol" /><br/>
            <img class="miniatura_classe" tal:attributes="src dades_objecte/thumbnail_classe" /><br/>
            <!-- TODO: on va link? -->
            <a href="#"><span class="titol_objecte georgia11c333333" tal:content="dades_objecte/titol" /></a>
            <ul tal:define="seccions dades_objecte/seccions">
                <tal:rep tal:repeat="dada_header seccions/header">
                    <li tal:attributes="class string:${dada_header/tipus} georgia11c999999" tal:content="dada_header/valor" />
                </tal:rep>
            </ul>
        </a>
    </tal:cond>
</div>