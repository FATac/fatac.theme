<tal:def tal:define="dades_objecte view/getDadesObjecte">
    <div class="genericview_fitxa_cerca" tal:condition="dades_objecte" tal:attributes="id dades_objecte/id">
        <div class="thumbnail">
                <img class="miniatura_objecte" tal:attributes="src view/getThumbnailObjecte; alt dades_objecte/titol" />
        </div>
        <div class="dades" tal:define="seccions dades_objecte/seccions">
            <img class="miniatura_classe" tal:attributes="src dades_objecte/thumbnail_classe" /><br/>
            <!-- TODO: destacar primer element i afegir link (on va link?) -->
            <tal:rep tal:repeat="dada_header seccions/header">
                <span tal:attributes="class string:${dada_header/tipus} georgia11c999999" tal:content="dada_header/valor" />
            </tal:rep>
        </div>
    </div>
</tal:def>