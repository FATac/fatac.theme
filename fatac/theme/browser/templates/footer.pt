                <a class="georgia13c666666italic" tal:attributes="href linkabout/enllac; title linkabout/descripcio" tal:content="linkabout/titol" /> |
            </tal:co>
            <tal:co tal:condition="linkcomfunciona">
                <a class="georgia13c666666italic" tal:attributes="href linkcomfunciona/enllac; title linkcomfunciona/descripcio" tal:content="linkcomfunciona/titol" /> |
            </tal:co>
            <tal:co tal:condition="linkcredits">
                <a class="georgia13c666666italic" tal:attributes="href linkcredits/enllac; title linkcredits/descripcio" tal:content="linkcredits/titol" /> |
            </tal:co>
            <tal:co tal:condition="linkavislegal">
                <a class="georgia13c666666italic" tal:attributes="href linkavislegal/enllac; title linkavislegal/descripcio" tal:content="linkavislegal/titol" />
            </tal:co>
